<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="pinup_price_puchase_tree_view">
            <field name="name">pinup.price.purchase.tree</field>
            <field name="model">pinup.price.purchase</field>
            <field name="arch" type="xml">
                <tree string="Waterhole Hired Tree" editable="bottom">
                    <field name="request_date"/>
                    <field name="pinup_tons"/>
                    <field name="price"/>
                    <field name="tc"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="purchase_order_pinup_price">
            <field name="name">purchase.order.pinup.price</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page string="Pin up">
                        <field name="pinup_price_purchase"/>
                    </page>
                </notebook>
            </field>
        </record>
    </data>
</openerp>
